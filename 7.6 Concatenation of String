#include<iostream>
using namespace std;

int main(){

    string str1=" ";
    string str2=" ";
    string str_con=" ";

    cout<<"Enter input for String1: ";
    cin>>str1;

    cout<<"Enter input for String2: ";
    cin>>str2;

    str_con=str1+" "+str2;
    cout<<"Concatenated String is: "<<str_con;

    return 0;
}


/*         OUTPUT
Enter input for String1: Shwetal
Enter input for String2: Koranne
Concatenated String is: Shwetal Koranne
*/
